<?xml version="1.0" encoding="utf-8"?>
<domain xmlns="http://cayenne.apache.org/schema/10/domain"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/10/domain http://cayenne.apache.org/schema/10/domain.xsd"
	 project-version="10">
	<map name="datamap"/>
	<node name="datanode"
		 factory="org.apache.cayenne.configuration.server.XMLPoolingDataSourceFactory"
		 schema-update-strategy="org.apache.cayenne.access.dbsync.SkipSchemaUpdateStrategy">
		<map-ref name="datamap"/>
		<data-source>
			<driver value="org.postgresql.Driver"/>
			<url value="jdbc:postgresql://localhost:5432/database"/>
			<connectionPool min="1" max="1"/>
			<login userName="postgres" password="postgres"/>
		</data-source>
	</node>
</domain>
